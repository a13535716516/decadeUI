/* 牌堆 */
.cardRoundNumber {
	top: 5px;
	left: 25px;
	font-weight:400;
	font-family: 'yuanli';
	z-index: 10;
	/*text-shadow: #B4A993 0.2px 0.2px 0.2px,
		#B4A993 0.2px -0.2px 0.2px,
		#B4A993 -0.2px 0.2px 0.2px,
		#B4A993 -0.2px -0.2px 0.2px;*/
}
.cardRoundNumber > .cardPileNumber {
	top: -15px;
	--w: 65px;
	--h: calc(var(--w) * 173/ 127);
	width: var(--w);
	height: var(--h);
	background-image: url("./images/uibutton/cardback3a.png");
	background-size: 100% 100%;
	background-color: transparent;
	position: relative;
	 
}
.cardRoundNumber > .cardPileNumber > span {
	left: 8px;/*牌堆数右移，越大越靠右*/
	top: 5px;
	width: 100%;
	display: block;
	position: absolute;
	line-height: 20px;
	text-align: center;
	color:#b18d43;
	font-size: 14px;
	font-weight: bold;
}
.cardRoundNumber > .roundNumber,
.cardRoundNumber > .time {/*游戏时间*/
	left: 10px;/*牌堆数/时间右移，越大越靠右*/
	top: 24px;
	height: 20px;
	line-height: 16px;/*游戏轮数/时间间隔*/
	white-space: nowrap;
	color: #f9e1b8;
	z-index: 3;
}
.cardRoundNumber > .time {/*游戏时间*/
	top: 50px;/*高度*/
	left: 16px;
	font-size: 14px;
	color: #c1a278;
	font-weight: bold;
}

