/*-----------------------------*/

.lbtn-confirm:not(.closing).removing {
	opacity: 1 !important;
}
.control {
	--height: 32px;
	width: auto !important;
	background: transparent;
	box-shadow: none;
	line-height: normal;
	padding: 0;
}



/*无懈按钮位置*/
#arena>#dui-controls>.control.stayleft {
	left: calc(50% - 420px) !important;
	margin-bottom: 3px !important;
	order: 3;
}


.control > div.disabled:not([data-type="endButton"]) {
	background-size: 100% 100%;
}

.control > div.disabled:not([data-type="endButton"]) {
	-webkit-filter: grayscale(100%);
	opacity: 1 !important;
	/*-webkit-filter: brightness(70%);*/
}

.lbtn-confirm[data-skill] > [data-type="skill2"] {
	display: none;
}





/*出牌记录*/
#arena.chess>#arenalog {
display: none !important;
background:none ;
}
#arena.observe .handcards>.card>div {
opacity: 0 !important;
background:none;
}
#arena.hide_name .player>.name:not(.name_seat),
#arena.hide_name .player>.name2,
#arena.hide_name>.dialog .button.character>.name{
display: none !important;
background:none;
}

#arenalog {
width: calc(50% - 180px);
height: calc(100% - 200px);/*提示条内容高度*/
left: calc(50% + 60px);
overflow: hidden;
font-family: 'yuanli' !important;
z-index:10;
pointer-events: none;
text-align: center;
animation-name:uimove;
animation-duration: 2s;
animation-timing-function:ease;
animation-iteration-count: /*infinite;*/2;
}
/*提示条高度，越大越往下*/
@keyframes uimove{
   from{top:280px;
        opacity:1.5;
   }
   
   to{top:150px;
      opacity:0.5;
   }
}

#arenalog.withdialog {
opacity: 0.5;
}
#arena:not(.oldlayout)>#arenalog[data-position="center"] {
left: calc(25% + 80px);/*提示条左右位置*/
}
#arena:not(.oldlayout)>#arenalog[data-position="left"] {
left: 150px;
}
#arena.oldlayout>#arenalog {
top: 160px;
width: calc(70% - 300px);
height: calc(100% - 325px);
}
#arena.oldlayout>#arenalog[data-position="center"] {
left: calc(35% + 150px);
}
#arena.oldlayout>#arenalog[data-position="left"] {
left: 240px;
}

#arenalog>div {
position: relative;
display: block;
width: calc(100% - 21px);
left: 20px;
line-height: 21px;
font-size: 18px;
letter-spacing: 3px;/*字间距*/
font-weight: 500;/*字粗细*/
text-shadow: -1.5px 0px 2.5px black, 0px -1.5px 2.5px black, 1.5px 0px 2.5px black ,0px 1.5px 2.5px black;
background: linear-gradient(to right, transparent 0%, rgba(0,0,0,0.5) 10%, rgba(0,0,0,0.5) 90%, transparent 100%);
}
#window:not(.low_performance) #arena #arenalog>div {
animation: game_start 1s;
-webkit-animation: game_start 1s;
}
